{
  "name": "gh-label-bot-ts",
  "version": "1.0.0",
  "private": true,
  "description": "A PR labeler GitHub App built with Probot.",
  "author": "John Hancock <jhancock93@users.noreply.github.com>",
  "license": "ISC",
  "repository": "https://github.com/jhancock93/gh-labelbot-ts.git",
  "homepage": "https://github.com/jhancock93/gh-label-bot-ts",
  "bugs": "https://github.com/jhancock93/gh-label-bot-ts/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc && (tsc -w --preserveWatchOutput & nodemon)",
    "dev": "npm run build:watch",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "ignore": "^3.3.3",
    "probot": "^9.11.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "@types/node": "^13.1.0",
    "@types/joi": "^14.3.4",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^25.2.1",
    "nock": "^12.0.0",
    "nodemon": "^2.0.0",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1",
    "ts-jest": "^25.1.0",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}